    "release": "pnpm update-version && pnpm publish-pkg",
    "sync-pkg": "esno ./scripts/sync.ts",
    "prepare": "pnpm -r run stub && husky install"
  },
  "dependencies": {
    "fast-glob": "^3.2.12"
  },
  "devDependencies": {
    "@soybeanjs/cli": "^0.1.2",
