{
  "name": "@soybeanjs/vite-plugin-vue-page-route",
  "version": "0.0.1",
  "description": "A vite plugin for vue, auto generate route info by page",
  "author": {
    "name": "Soybean",
    "email": "honghuangdc@gmail.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/vite-plugin-vue-page-route",
  "repository": {
    "url": "https://github.com/soybeanjs/vite-plugin-vue-page-route.git"
  },
  "keywords": [
    "soybeanjs",
    "soybean",
    "vite",
    "vite-plugin",
    "vite-plugin-vue",
    "vite-plugin-vue-page-route",
    "page-route",
    "directory-route",
    "view-route"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --fix",
    "commit": "soybean git-commit",
    "esno": "esno",
    "cleanup": "esno ./scripts/cleanup.ts",
    "update-pkg": "ncu --deep -u",
    "update-version": "bumpp package.json",
    "publish-pkg": "pnpm -r publish --access public",
    "release": "pnpm update-version && pnpm publish-pkg",
    "sync-pkg": "esno ./scripts/sync.ts",
    "prepare": "pnpm -r run stub"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "execa": "5.1.1",
    "fast-glob": "^3.2.12",
    "kolorist": "^1.6.0",
    "magic-string": "^0.27.0",
    "micromatch": "^4.0.5"
  },
  "devDependencies": {
    "@soybeanjs/cli": "^0.1.4",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.11.11",
    "@types/throttle-debounce": "^5.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "eslint-config-soybeanjs": "^0.2.1",
    "esno": "^0.16.3",
    "lint-staged": "^13.1.0",
    "npm-check-updates": "^16.5.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "zx": "^7.1.1"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soybean git-commit-verify",
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  }
}
